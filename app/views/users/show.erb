<h1><%= @user.name %></h1>
<h2> <%= @user.address %> </h2>
<h3>Listed Items:</h3>
<% @user.items.each do |item| %>
  <%= item.name %><br>
  <a href="<%=item_path(item)%>"> <img src="<%= item.images.first.url %>" alt="" ></a><br>
  <%= item.description %><br>
  <%= item.available? %> <br>
<% end %>

<h3>Most Recent Reviews</h3>
<% @user.five_recent_reviews.each do |review| %>
<%= review.rating %> <br>
<%= review.content %> <br>
<% end %>
