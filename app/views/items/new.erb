<h1>List a New Item</h1>

<%= form_for @item do |f| %>
  <%= f.label :name %><%= f.text_field :name %>
  <%= f.label :description %><%= f.text_field :description %>
  <%= f.label :price %><%= f.number_field :price %>
    <%= f.fields_for :images do |ff| %>
      <%= ff.label :url %>
      <%= ff.text_field :url %>
      <% end %>
  <%= f.submit "Post Item"  %>
  <% end %>
